{
  "name": "test-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.1",
    "@angular/common": "^12.1.5",
    "@angular/compiler": "^12.1.5",
    "@angular/core": "^12.1.5",
    "@angular/forms": "~12.1.1",
    "@angular/platform-browser": "^12.1.5",
    "@angular/platform-browser-dynamic": "^12.1.5",
    "@angular/router": "~12.1.1",
    "core-js": "^3.16.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@angular/elements": "^12.1.4",
    "@babel/core": "^7.14.8",
    "@compodoc/compodoc": "^1.1.14",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/angular": "^6.3.6",
    "@storybook/builder-webpack5": "^6.3.6",
    "@storybook/manager-webpack5": "^6.3.6",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.19",
    "@webcomponents/custom-elements": "^1.4.3",
    "babel-loader": "^8.2.2",
    "creevey": "^0.7.36",
    "jest": "^27.0.6",
    "jest-preset-angular": "^9.0.5",
    "karma-coverage": "~2.0.3",
    "typescript": "^4.3.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [ "<rootDir>/src/" ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ]
  }
}
